{
    "blocks": false,
    "brackets": {
        "expect": "never",
        "error": true
    },
    "colons": {
        "expect": "never",
        "error": true
    },
    "colors": false,
    "commaSpace": {
        "expect": "always",
        "error": true
    },
    "commentSpace": "always",
    "cssLiteral": "never",
    "customProperties": [],
    "depthLimit": false,
    "duplicates": {
        "expect": true,
        "error": true
    },
    "efficient": "always",
    "exclude": ["node_modules/**/*"],
    "extendPref": false,
    "globalDupe": {
        "expect": true,
        "error": true
    },
    "groupOutputByFile": true,
    "indentPref": false,
    "leadingZero": {
        "expect": "never",
        "error": true
    },
    "maxErrors": false,
    "maxWarnings": false,
    "mixed": false,
    "mixins": [],
    "namingConvention": {
        "expect": "BEM",
        "error": true
    },
    "namingConventionStrict": {
        "expect": true,
        "error": true
    },
    "none": {
        "expect": "never",
        "error": true
    },
    "noImportant": {
        "expect": true,
        "error": true
    },
    "parenSpace": false,
    "placeholders": "always",
    "prefixVarsWithDollar": {
        "expect": "always",
        "error": true
    },
    "quotePref": {
        "expect": "single",
        "error": true
    },
    "reporterOptions": {
        "columns": ["lineData", "severity", "description", "rule"],
        "columnSplitter": "  ",
        "showHeaders": false,
        "truncate": true
    },
    "semicolons": {
        "expect": "never",
        "error": true
    },
    "sortOrder": "content",
    "stackedProperties": {
        "expect": "never",
        "error": true
    },
    "trailingWhitespace": "never",
    "universal": false,
    "valid": true,
    "zeroUnits": {
        "expect": "never",
        "error": true
    },
    "zIndexNormalize": false
}
